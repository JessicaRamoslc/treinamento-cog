     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Digite o valor em Celsius: 10\n",
      "O valor em Farenheit é: 50.0\n"
     ]
    }
   ],
   "source": [
    "celsius = int(input(\"Digite o valor em Celsius: \"))\n",
    "F = (celsius * 9/5) + 32\n",
    "print(\"O valor em Farenheit é: {}\".format(F))"
